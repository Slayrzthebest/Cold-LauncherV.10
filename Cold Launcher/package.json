{
  "name": "cold-launcher",
  "version": "1.0.0",
  "description": "Cold Launcher en Electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "author": "Cold",
  "license": "MIT",
  "devDependencies": {
    "electron": "^29.3.0",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.cold.launcher",
    "productName": "Cold",
    "files": [
      "assets/**/*",
      "main.js",
      "preload.js",
      "renderer.js",
      "index.html",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/logo.ico"
    }
  }
}
